{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "DEV_API_KEY": "@dev_api_key",
    "DEV_SECRET_KEY": "@dev_secret_key",
    "DEV_SECRET_VALUE": "@dev_secret_value"
  }
}
